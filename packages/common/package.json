{
  "name": "@gympass/yoga-common",
  "version": "1.1.1",
  "description": "Shared resources between packages",
  "homepage": "https://github.com/Gympass/yoga",
  "license": "MIT",
  "types": "dist/index.d.ts",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --legacy-output",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "lint": "eslint ./src",
    "prebuild": "rm -rf ./dist",
    "prepublishOnly": "node ../../scripts/prepublish.js"
  },
  "devDependencies": {
    "@gympass/ts-config": "*",
    "tsup": "^6.2.3",
    "typescript": "^4.8.4"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gympass/yoga.git"
  },
  "bugs": {
    "url": "https://github.com/Gympass/yoga/issues"
  }
}
