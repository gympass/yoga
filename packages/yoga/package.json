{
  "name": "@gympass/yoga",
  "version": "7.53.7",
  "description": "Gympass component library",
  "main": "./src",
  "sideEffects": false,
  "scripts": {
    "build": "yarn build:web && yarn build:native && yarn build:esm",
    "build:web": "NODE_ENV=web babel ./src --out-dir dist/cjs",
    "build:esm": "NODE_ENV=esm babel ./src --out-dir dist/esm",
    "build:native": "NODE_ENV=native babel ./src --out-dir dist/cjs",
    "prebuild": "rm -rf ./dist",
    "postbuild": "cp -r ./src/Fonts ./dist/fonts",
    "prepublishOnly": "node ../../scripts/prepublish.js --rn",
    "lint": "eslint ./src",
    "test": "NODE_ENV=test jest --config=../../jest/config/web.js && jest --config=../../jest/config/native.js",
    "test:web": "NODE_ENV=test jest --config=../../jest/config/web.js",
    "test:native": "NODE_ENV=test jest --config=../../jest/config/native.js"
  },
  "keywords": [
    "Gympass",
    "components",
    "styled-components",
    "react",
    "design-system"
  ],
  "homepage": "https://github.com/Gympass/yoga#readme",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gympass/yoga.git"
  },
  "bugs": {
    "url": "https://github.com/Gympass/yoga/issues"
  },
  "dependencies": {
    "@gympass/yoga-common": "*",
    "@gympass/yoga-helpers": "*",
    "@gympass/yoga-icons": "*",
    "@gympass/yoga-illustrations": "*",
    "@gympass/yoga-system": "*",
    "@gympass/yoga-tokens": "*",
    "@ptomasroos/react-native-multi-slider": "^1.0.0",
    "@radix-ui/react-dropdown-menu": "^0.1.6",
    "date-fns": "^2.29.2",
    "downshift": "^5.0.0",
    "lodash.get": "^4.4.2",
    "prop-types": "^15.7.2",
    "proxy-polyfill": "^0.3.1",
    "rc-slider": "^8.7.1",
    "react-google-font-loader": "^1.1.0"
  },
  "devDependencies": {
    "@testing-library/react": "^12.0.4",
    "@testing-library/react-native": "^9.1.0",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "styled-components": "^4.4.0"
  },
  "peerDependencies": {
    "@react-native-picker/picker": "^1.16.6",
    "react": ">=16",
    "styled-components": "^4.4.0"
  }
}
